							"    pm.response.to.have.status(204);",
							"});",
							"",
							"pm.sendRequest({",
							"    url: pm.environment.get(\"app_url\") + \"/loads/\" + pm.environment.get(\"load_id\"),",
							"}, function(err, res) {",
							"   if (err) {",
							"       console.log(err);",
							"   } else {",
							"       console.log(res.json());",
							"       pm.test(\"load removed\", function () {",
							"           pm.expect(res.json().carrier).to.not.exist &&",
							"           pm.response.to.have.status(204)",
							"       });",
							"   }",
							"});"
						],
